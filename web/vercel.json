{
    "version": 2,
    "builds": [
      {
        "src": "web/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      },
      {
        "src": "build_file.sh",
        "use": "@vercel/static",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "web/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles"
  }