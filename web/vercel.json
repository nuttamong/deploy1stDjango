{
    "builds": [
        {
          "src": "web/asgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
        },
        {
          "src": "build_files.sh",
          "use": "@vercel/static"
        }
      ],
      "routes": [
        {
          "src": "/staticfiles/(.*)",
          "dest": "/staticfiles/$1"
        },
        {
          "src": "/(.*)",
          "dest": "web/asgi.py"
        }
      ]
}